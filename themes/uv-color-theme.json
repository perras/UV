			"settings": {
				"foreground": "#f1ff33"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#deffab"
			}
		},
		{
			"name": "Function, Special Method",
